{
  "name": "Medium Automatic Sign-In",
  "description": "Automatic sign-in for Medium-based publications.",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png",
    "256": "icon256.png"
  },
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["index.js"]
    }
  ]
}
